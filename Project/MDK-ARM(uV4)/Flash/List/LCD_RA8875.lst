N	*/
N	#define RA8875_CS_0()	GPIOI->BSRRH = GPIO_Pin_10
N	#define RA8875_CS_1()	GPIOI->BSRRL = GPIO_Pin_10
N
N	#define SPI_WRITE_DATA	0x00
N	#define SPI_READ_DATA	0x40
N	#define SPI_WRITE_CMD	0x80

N*			_usColor  :像素颜色
N*	返 回 值: RGB颜色值
N*********************************************************************************************************
N*/
Nuint16_t RA8875_GetPixel(uint16_t _usX, uint16_t _usY)
N{
N	uint16_t usRGB;
N
N//	RA8875_WriteReg(0x40, (1 << 0));	/* 设置为绘图模式，读取光标不自动加1 */
N
N	RA8875_SetReadCursor(_usX, _usY);	/* 设置读取光标位置 */
N
N	s_ucRA8875Busy = 1;
N
N	RA8875_WriteCmd(0x02);
N	usRGB = RA8875_ReadData16();	/* 第1次读取数据丢弃 */
N	usRGB = RA8875_ReadData16();
N	usRGB = RA8875_ReadData16();
N
N	s_ucRA8875Busy = 0;
N
N	return usRGB;
N}

S	    //Vertical pixels = VDHR + 1
S		RA8875_WriteReg(0x19, 0xDF);
S
S	    //VDHR1 //Vertical Display Height Bit [8]
S	    //Vertical pixels = VDHR + 1
S		RA8875_WriteReg(0x1A, 0x01);
S
S	    //VNDR0 //Vertical Non-Display Period Bit [7:0]
S	    //Vertical Non-Display area = (VNDR + 1)
S		RA8875_WriteReg(0x1B, 0x0A);
S
S	    //VNDR1 //Vertical Non-Display Period Bit [8]
S	    //Vertical Non-Display area = (VNDR + 1)
S		RA8875_WriteReg(0x1C, 0x00);
S
S	    //VSTR0 //VSYNC Start Position[7:0]
S	    //VSYNC Start Position(PCLK) = (VSTR + 1)
S		RA8875_WriteReg(0x1D, 0x0E);
S
S	    //VSTR1 //VSYNC Start Position[8]
S	    //VSYNC Start Position(PCLK) = (VSTR + 1)
S		RA8875_WriteReg(0x1E, 0x00);
S
S	    //VPWR //VSYNC Polarity ,VSYNC Pulse Width[6:0]
S	    //VSYNC Pulse Width(PCLK) = (VPWR + 1)
S	    RA8875_WriteReg(0x1F, 0x01);
N	#endif
N	}
N
N	/* 设置TFT面板的 DISP  引脚为高，使能面板. 安富莱TFT模块的DISP引脚连接到RA8875芯片的GP0X脚 */
N	RA8875_WriteReg(0xC7, 0x01);	/* DISP = 1 */
N
N	/* LCD 显示/关闭讯号 (LCD Display on) */
N	RA8875_WriteReg(0x01, 0x80);
N
N	/* 	REG[40h] Memory Write Control Register 0 (MWCR0)
N
N		Bit 7	显示模式设定
N			0 : 绘图模式。
N			1 : 文字模式。
N
N		Bit 6	文字写入光标/内存写入光标设定
N			0 : 设定文字/内存写入光标为不显示。
N			1 : 设定文字/内存写入光标为显示。
N
N		Bit 5	文字写入光标/内存写入光标闪烁设定
N			0 : 游标不闪烁。
N			1 : 游标闪烁。
N
N		Bit 4   NA
N
N		Bit 3-2  绘图模式时的内存写入方向
N			00b : 左 -> 右，然后上 -> 下。
N			01b : 右 -> 左，然后上 -> 下。
N			10b : 上 -> 下，然后左 -> 右。
N			11b : 下 -> 上，然后左 -> 右。
N
N		Bit 1 	内存写入光标自动增加功能设定
N			0 : 当内存写入时光标位置自动加一。
N			1 : 当内存写入时光标位置不会自动加一。
N
N		Bit 0 内存读取光标自动增加功能设定
N			0 : 当内存读取时光标位置自动加一。
N			1 : 当内存读取时光标位置不会自动加一。
N	*/
N	//RA8875_WriteReg(0x40, 0x00);	/* 选择绘图模式 */	
N	RA8875_SetDirection(0);
N
N	/* 	REG[41h] Memory Write Control Register1 (MWCR1)
N		写入目的位置，选择图层1
N	*/
N	RA8875_WriteReg(0x41, 0x00);	/* 选择绘图模式, 目的为CGRAM */
N
N	RA8875_SetDispWin(0, 0, g_LcdHeight, g_LcdWidth);
N
N	if ((g_RA8875_IF == RA_HARD_SPI) || (g_RA8875_IF == RA_SOFT_SPI))
N	{
N		RA8875_HighSpeedSPI();		/* 配置完毕后，切换SPI到高速模式 */
N	}
N	else
N	{
N		#ifdef WRTIE_SF_EN		/* 使能了字库图片芯片写功能，配置CPU的SPI接口和PWM-GPIO */
N			bsp_InitRA8875Flash();		
N		#endif	
N	}
N}
N
N/*
N*********************************************************************************************************
N*	函 数 名: RA8875_SetDirection
N*	功能说明: 设置显示方向。
N*	形    参:  _ucDir : 显示方向代码 0 横屏正常, 1=横屏180度翻转, 2=竖屏, 3=竖屏180度翻转
N*	返 回 值: 无
N*********************************************************************************************************
N*/
Nvoid RA8875_SetDirection(uint8_t _ucDir)
N{
N	uint8_t reg20H = 0;
N	
N	s_reg_40H = 0x01;
N	
N	if (_ucDir == 0)
N	{
N		s_reg_40H = 0x01;
N		reg20H = (0 << 3) | (0 << 2);
N	}
N	else if (_ucDir == 1)
N	{
N		s_reg_40H = 0x01;	/* 2015-04-21 由05修改为 01. 解决emwin下的问题 */
N		reg20H = (1 << 3) | (1 << 2);
N	}
N	else if (_ucDir == 2)
N	{
N		s_reg_40H = 0x09;
N		reg20H = (0 << 3) | (1 << 2);
N	}
N	else if (_ucDir == 3)
N	{
N		s_reg_40H = 0x09;
N		reg20H = (1 << 3) | (0 << 2);
N	}
N	RA8875_WriteReg(0x40, s_reg_40H);	
N
N	/* 
N		REG[20h] Display Configuration Register (DPCR) 
N		Bit  说  明  初始值  Access
N		Bit7 
N			图层设定(Layer Control) 
N			0 : 单图层。
N			1 : 双图层。
N		Bit3  HDIR 水平扫描方向设定(n = SEG number) 
N			0 : 由SEG0 到SEG(n-1)。
N			1 : 由SEG(n-1) 到SEG0。
N		Bit2 VDIR 垂直扫描方向设定(n = COM number) 
N			0 : 由COM0 到COM(n-1)。
N			1 : 由COM(n-1) 到COM0。
N	*/
N	RA8875_WriteReg(0x20, reg20H);	
N
N	if (_ucDir > 1)	/* 竖屏  */
N	{
N		uint16_t temp;
N		
N		if (g_LcdHeight < g_LcdWidth)
N		{
N			temp = g_LcdHeight;
N			g_LcdHeight = g_LcdWidth;
N			g_LcdWidth = temp;
N		}
N	}		
N	else	/* 横屏 */
N	{
N		uint16_t temp;
N		
N		if (g_LcdHeight > g_LcdWidth)
N		{
N			temp = g_LcdHeight;
N			g_LcdHeight = g_LcdWidth;
N			g_LcdWidth = temp;
N		}
N	}
N}
N
N/*
N*********************************************************************************************************
N*	函 数 名: BTE_SetTarBlock
N*	功能说明: 设置RA8875 BTE目标区块以及目标图层
N*	形    参:
N*			uint16_t _usX : 水平起点坐标
N*			uint16_t _usY : 垂直起点坐标
N*			uint16_t _usHeight : 区块高度
N*			uint16_t _usWidth : 区块宽度
N*			uint8_t _ucLayer : 0 图层1； 1 图层2
N*	返 回 值: 无
N*********************************************************************************************************
N*/
Nstatic void BTE_SetTarBlock(uint16_t _usX, uint16_t _usY, uint16_t _usHeight, uint16_t _usWidth, uint8_t _ucLayer)
N{
N	/* 设置起点坐标 */
N	RA8875_WriteReg(0x58, _usX);
N	RA8875_WriteReg(0x59, _usX >> 8);
N
N	RA8875_WriteReg(0x5A, _usY);
N	if (_ucLayer == 0)	/* 图层2 */
N	{
N		RA8875_WriteReg(0x5B, _usY >> 8);
N	}
N	else
N	{
N		RA8875_WriteReg(0x5B, (1 << 7) | (_usY >> 8));	/* Bit7 表示图层， 0 图层1； 1 图层2*/
N	}
N
N	/* 设置区块宽度 */
N	RA8875_WriteReg(0x5C, _usWidth);
N	RA8875_WriteReg(0x5D, _usWidth >> 8);
N
N	/* 设置区块高度 */
N	RA8875_WriteReg(0x5E, _usHeight);
N	RA8875_WriteReg(0x5F, _usHeight >> 8);
N}

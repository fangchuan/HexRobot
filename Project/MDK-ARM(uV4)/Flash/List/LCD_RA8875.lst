N/*
N*********************************************************************************************************
N*	函 数 名: RA8875_Sleep
N*	功能说明: RA8875进入睡眠模式（同时关闭显示器）
N*	形    参:  无
N*	返 回 值: 无
N*********************************************************************************************************
N*/
Nvoid RA8875_Sleep(void)
N{
N	RA8875_SetBackLight(0);		/* 关闭背光 */
N
N	/* 设置TFT面板的 DISP  引脚为高，使能面板. 安富莱TFT模块的DISP引脚连接到RA8875芯片的GP0X脚 */
N	RA8875_WriteReg(0xC7, 0x00);	/* DISP = 0  */
N
N	RA8875_WriteReg(0x01, 0x01);
N	RA8875_WriteReg(0x01, 0x00);
N
N	RA8875_WriteReg(0x01, 0x02);	/* RA8875 Sleep */
N}
N
N/*
N*********************************************************************************************************
N*	函 数 名: RA8875_PutPixel
N*	功能说明: 画1个像素
N*	形    参:
N*			_usX,_usY : 像素坐标
N*			_usColor  :像素颜色
N*	返 回 值: 无
N*********************************************************************************************************
N*/
Nvoid RA8875_PutPixel(uint16_t _usX, uint16_t _usY, uint16_t _usColor)
N{	
N	if (g_RA8875_IF == RA_HARD_8080_16)	/* 8080硬件总线16bit */
N	{
N		// 优化 RA8875_SetCursor(_usX, _usY);
N		s_ucRA8875Busy = 1;
N		{	
N			if (g_LcdDirection > 1)	/* 竖屏  */
N			{
N				RA8875_REG = 0x46; RA8875_RAM = _usY;
X				*(volatile uint16_t *)(((uint32_t)(0x6C000000 | 0x00000000)) + (1 << (18 + 1))) = 0x46; *(volatile uint16_t *)(((uint32_t)(0x6C000000 | 0x00000000))) = _usY;
N				RA8875_REG = 0x47; RA8875_RAM = _usY >> 8;
X				*(volatile uint16_t *)(((uint32_t)(0x6C000000 | 0x00000000)) + (1 << (18 + 1))) = 0x47; *(volatile uint16_t *)(((uint32_t)(0x6C000000 | 0x00000000))) = _usY >> 8;
N				RA8875_REG = 0x48; RA8875_RAM = _usX;
X				*(volatile uint16_t *)(((uint32_t)(0x6C000000 | 0x00000000)) + (1 << (18 + 1))) = 0x48; *(volatile uint16_t *)(((uint32_t)(0x6C000000 | 0x00000000))) = _usX;
N				RA8875_REG = 0x49; RA8875_RAM = _usX >> 8;
X				*(volatile uint16_t *)(((uint32_t)(0x6C000000 | 0x00000000)) + (1 << (18 + 1))) = 0x49; *(volatile uint16_t *)(((uint32_t)(0x6C000000 | 0x00000000))) = _usX >> 8;
N			}
N			else	/* 横屏 */
N			{
N				RA8875_REG = 0x46; RA8875_RAM = _usX;
X				*(volatile uint16_t *)(((uint32_t)(0x6C000000 | 0x00000000)) + (1 << (18 + 1))) = 0x46; *(volatile uint16_t *)(((uint32_t)(0x6C000000 | 0x00000000))) = _usX;
N				RA8875_REG = 0x47; RA8875_RAM = _usX >> 8;
X				*(volatile uint16_t *)(((uint32_t)(0x6C000000 | 0x00000000)) + (1 << (18 + 1))) = 0x47; *(volatile uint16_t *)(((uint32_t)(0x6C000000 | 0x00000000))) = _usX >> 8;
N				RA8875_REG = 0x48; RA8875_RAM = _usY;
X				*(volatile uint16_t *)(((uint32_t)(0x6C000000 | 0x00000000)) + (1 << (18 + 1))) = 0x48; *(volatile uint16_t *)(((uint32_t)(0x6C000000 | 0x00000000))) = _usY;
N				RA8875_REG = 0x49; RA8875_RAM = _usY >> 8;	
X				*(volatile uint16_t *)(((uint32_t)(0x6C000000 | 0x00000000)) + (1 << (18 + 1))) = 0x49; *(volatile uint16_t *)(((uint32_t)(0x6C000000 | 0x00000000))) = _usY >> 8;	
N			}
N		}
N		
N		// 优化 RA8875_WriteCmd(0x02); 		/* 用于设定RA8875 进入内存(DDRAM或CGRAM)读取/写入模式 */
N		// 优化 RA8875_WriteData16(_usColor);
N		
N		RA8875_REG = 0x02;
X		*(volatile uint16_t *)(((uint32_t)(0x6C000000 | 0x00000000)) + (1 << (18 + 1))) = 0x02;
N		RA8875_RAM = _usColor;
X		*(volatile uint16_t *)(((uint32_t)(0x6C000000 | 0x00000000))) = _usColor;
N
N		s_ucRA8875Busy = 0;
N	}
N	else	/* SPI接口分支 */
N	{
N		RA8875_SetCursor(_usX, _usY);
N		s_ucRA8875Busy = 1;
N
N		RA8875_WriteCmd(0x02); 		/* 用于设定RA8875 进入内存(DDRAM或CGRAM)读取/写入模式 */
N		RA8875_WriteData16(_usColor);
N
N		s_ucRA8875Busy = 0;
N	}
N}

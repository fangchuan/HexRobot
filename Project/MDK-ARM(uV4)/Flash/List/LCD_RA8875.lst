N/*
N*********************************************************************************************************
N*	函 数 名: RA8875_GetPixelGUI
N*	功能说明: 读取1个像素
N*	形    参:
N*			_usX,_usY : 像素坐标
N*			_usColor  :像素颜色
N*	返 回 值: RGB颜色值
N*********************************************************************************************************
N*/
Nuint16_t RA8875_GetPixelGUI(uint16_t _usX, uint16_t _usY)
N{
N	uint16_t usRGB;
N	
N	s_ucRA8875Busy = 1;
N
N//	RA8875_WriteReg(0x40, (1 << 0));	/* 设置为绘图模式，读取光标不自动加1 */
N//  RA8875_SetReadCursor(_usX, _usY);	/* 设置读取光标位置 */
N	
N	RA8875_REG = 0x4A; RA8875_RAM = _usX;
X	*(volatile uint16_t *)(((uint32_t)(0x6C000000 | 0x00000000)) + (1 << (18 + 1))) = 0x4A; *(volatile uint16_t *)(((uint32_t)(0x6C000000 | 0x00000000))) = _usX;
N	RA8875_REG = 0x4B; RA8875_RAM = _usX >> 8;
X	*(volatile uint16_t *)(((uint32_t)(0x6C000000 | 0x00000000)) + (1 << (18 + 1))) = 0x4B; *(volatile uint16_t *)(((uint32_t)(0x6C000000 | 0x00000000))) = _usX >> 8;
N	RA8875_REG = 0x4C; RA8875_RAM = _usY;
X	*(volatile uint16_t *)(((uint32_t)(0x6C000000 | 0x00000000)) + (1 << (18 + 1))) = 0x4C; *(volatile uint16_t *)(((uint32_t)(0x6C000000 | 0x00000000))) = _usY;
N	RA8875_REG = 0x4D; RA8875_RAM = _usY >> 8;
X	*(volatile uint16_t *)(((uint32_t)(0x6C000000 | 0x00000000)) + (1 << (18 + 1))) = 0x4D; *(volatile uint16_t *)(((uint32_t)(0x6C000000 | 0x00000000))) = _usY >> 8;
N	RA8875_REG = 0x02;
X	*(volatile uint16_t *)(((uint32_t)(0x6C000000 | 0x00000000)) + (1 << (18 + 1))) = 0x02;
N	
N	usRGB = RA8875_RAM;
X	usRGB = *(volatile uint16_t *)(((uint32_t)(0x6C000000 | 0x00000000)));
N	usRGB = RA8875_RAM;
X	usRGB = *(volatile uint16_t *)(((uint32_t)(0x6C000000 | 0x00000000)));
N	usRGB = RA8875_RAM;
X	usRGB = *(volatile uint16_t *)(((uint32_t)(0x6C000000 | 0x00000000)));
N
N	s_ucRA8875Busy = 0;
N
N	return usRGB;
N}

N  __IO uint8_t  ECC;                  /*!< ECC code */
X  volatile uint8_t  ECC;                   
N  __IO uint8_t  CSD_CRC;              /*!< CSD CRC */
X  volatile uint8_t  CSD_CRC;               
N  __IO uint8_t  Reserved4;            /*!< always 1*/
X  volatile uint8_t  Reserved4;             

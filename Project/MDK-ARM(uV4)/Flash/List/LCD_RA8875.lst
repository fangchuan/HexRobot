N	RA8875_WriteCmd(0x90);
N	RA8875_WriteData( (1 << 6) | (0 << 5));
N//	while (RA8875_ReadReg(0x90) & (1 << 6));				/* 等待结束 */
N	RA8875_WaitBusy();
N	s_ucRA8875Busy = 0;

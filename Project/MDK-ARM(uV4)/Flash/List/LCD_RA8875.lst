N/*
N*********************************************************************************************************
N*	函 数 名: RA8875_ClrScr
N*	功能说明: 根据输入的颜色值清屏.RA8875支持硬件单色填充。该函数仅对当前激活的显示窗口进行清屏. 显示
N*			 窗口的位置和大小由 RA8875_SetDispWin() 函数进行设置
N*	形    参:  _usColor : 背景色
N*	返 回 值: 无
N*********************************************************************************************************
N*/
Nvoid RA8875_ClrScr(uint16_t _usColor)
N{
N	/* 也可以通过0x30-0x37寄存器获得获得当前激活的显示窗口 */
N
N	/* 单色填满功能, 中文pdf 第162页
N	此功能使用于将选定特定区域画面清除或是\入给定某种前景色，R8875 填入的单色设定为BTE 前景色。
N
N	操作步骤:
N		1. 设定目的图层和位置 REG[58h], [59h], [5Ah], [5Bh]
N		2. 设定BTE 宽度和高度 REG[5Ch], [5Dh], [5Eh], [5Fh]
N		3. 设定BTE 操作码和光栅运算码  REG[51h] Bit[3:0] = 0Ch
N		4. 设定前景色  REG[63h], [64h], [65h]
N		5. 开启BTE 功能  REG[50h] Bit7 = 1
N		6. 检查状态缓存器 STSR Bit6，确认BTE 是否完成
N	*/
N	BTE_SetTarBlock(s_WinX, s_WinY, s_WinHeight, s_WinWidth, 0);	/* 设置BTE位置和宽度高度以及目标图层（0或1） */
N	BTE_SetOperateCode(0x0C);		/* 设定BTE 操作码和光栅运算码  REG[51h] Bit[3:0] = 0Ch */
N	RA8875_SetFrontColor(_usColor);	/* 设置BTE前景色 */
N	BTE_Start();					/* 开启BTE 功能 */
N	BTE_Wait();						/* 等待操作结束 */
N}
N
N/*
N*********************************************************************************************************
N*	函 数 名: RA8875_DispBmpInFlash
N*	功能说明: 显示RA8875外挂串行Flash中的BMP位图.
N*			    位图格式:从左到右 从上到下扫描, 每个像素2字节, RGB = 565 格式, 高位在前.
N*	形    参:
N*			_usX, _usY : 图片的坐标
N*			_usHeight  :图片高度
N*			_usWidth   :图片宽度
N*			_uiFlashAddr       :串行Flash地址
N*	返 回 值: 无
N*********************************************************************************************************
N*/
Nvoid RA8875_DispBmpInFlash(uint16_t _usX, uint16_t _usY, uint16_t _usHeight, uint16_t _usWidth,
N	uint32_t _uiFlashAddr)
N{
N	/* pdf 179页 7-10-1 连续内存直接存取模式
N		1. 设定工作窗口范围 (REG[30h] ~REG[37h])和内存写入位置 (REG[46h] ~REG[49h])
N		2. 设定Serial Flash/ROM 组态 (REG[05h])
N		3. 设定 内存直接存取数据来源起始位置 (REG[B0h] ~REG[B2h])
N		4. 设定 内存直接存取区块宽度 (REG[B4h] 和REG[B5h])
N		5. 设定 内存直接存取区块高度 (REG[B6h] 和 REG[B7h])
N		6. 设定内存直接存取来源图片宽度 (REG[B8h] 和 REG[B9h])
N		7. 开启内存直接存取为区块搬移模式 (REG[BFh] bit 1)
N		8. 开启内存直接存取起始讯号且检查内存直接存取忙碌讯号 (REG[BFh] bit 0)
N	*/
N	RA8875_SetDispWin(_usX, _usY, _usHeight, _usWidth);	/* REG[30h] ~REG[37h] 和 REG[46h] ~REG[49h] */
N
N	/* 05H  REG[05h] Serial Flash/ROM Configuration Register (SROC)
N		7	Serial Flash/ROM I/F # 选择
N				0:选择Serial Flash/ROM 0 接口。[安富莱RA8875驱动板图库芯片接在 1 接口]
N				1:选择Serial Flash/ROM 1 接口。
N		6	Serial Flash/ROM 寻址模式
N				0: 24 位寻址模式。
N				此位必须设为0。
N		5	Serial Flash/ROM 波形模式
N				0: 波形模式 0。
N				1: 波形模式 3。
N		4-3	Serial Flash /ROM 读取周期 (Read Cycle)
N			00b: 4 bus ?? 无空周期 (No Dummy Cycle)。
N			01b: 5 bus ??1 byte 空周期。
N			1Xb: 6 bus ??2 byte 空周期。
N		2	Serial Flash /ROM 存取模式 (Access Mode)
N			0: 字型模式 。
N			1: DMA 模式。
N		1-0	Serial Flash /ROM I/F Data Latch 选择模式
N			0Xb: 单一模式。
N			10b: 双倍模式0。
N			11b: 双倍模式1。
N	*/
N	
N	/* 对于铁框屏，一片 W25Q128，前面2MB是字库。后面14MB是图片 */
N	if (g_tW25.ChipID == W25Q128)
N	{
N		_uiFlashAddr += PIC_OFFSET;	/* 前面2MB分配给字库 */	
X		_uiFlashAddr += (2*1024*1024);	 	
N		#ifdef FLASH_DUAL_SPEED_EN			/* 双速模式， MOSI和MISO同时读取数据 */
N			RA8875_WriteReg(0x05, (0 << 7) | (0 << 6) | (1 << 5) | (0 << 3) | (1 << 2) | (2 << 0));
N		#else				/* 普通模式，仅 MISO 读取数据 */
S			RA8875_WriteReg(0x05, (0 << 7) | (0 << 6) | (1 << 5) | (0 << 3) | (1 << 2) | (0 << 0));
N		#endif		
N	}	
N	else	/* 增强型屏，1片W25Q64(8MB)做字库，1片W25Q64(8MB)做图库 */
N	{
N		#ifdef FLASH_DUAL_SPEED_EN			/* 双速模式， MOSI和MISO同时读取数据 */
N			RA8875_WriteReg(0x05, (1 << 7) | (0 << 6) | (1 << 5) | (0 << 3) | (1 << 2) | (2 << 0));
N		#else				/* 普通模式，仅 MISO 读取数据 */
S			RA8875_WriteReg(0x05, (1 << 7) | (0 << 6) | (1 << 5) | (0 << 3) | (1 << 2) | (0 << 0));
N		#endif	
N	}
N
N	/*
N		Serial Flash/ROM 频率频率设定
N			0xb: SFCL 频率 = 系统频率频率(当DMA 为使能状态，并且色彩深度为256 色，则SFCL 频率
N				固定为=系统频率频率/ 2)
N			10b: SFCL 频率 =系统频率频率/ 2
N			11b: SFCL 频率 =系统频率频率/ 4
N
N		安富莱TFT驱动板。4.3寸系统频率为 68MHz； 7寸系统频率为 81.25MHz。
N
N		8M串行Flash的访问速度:SPI 时钟频率:80MHz(max.)
N		因此可以设置为 1 分频
N	*/
N	RA8875_WriteReg(0x06, (0 << 0));	/* 设置SCL时钟频率 */
N
N	/* 设定源地址
N		B0H   DMA 来源开始位置[7:0]
N		B1H   DMA 来源开始位置[15:8]
N		B2H   DMA 来源开始位置[23:16]
N	*/
N	RA8875_WriteReg(0xB0, (uint8_t)_uiFlashAddr);
N	RA8875_WriteReg(0xB1, (uint8_t)(_uiFlashAddr >> 8));
N	RA8875_WriteReg(0xB2, (uint8_t)(_uiFlashAddr >> 16));
N
N	/*
N		设置区块大小
N		B4   DMA 区块宽度[7:0]
N		B5   DMA 区块宽度[9:8]
N		B6   DMA 区块高度[7:0]
N	    B7   DMA 区块高度[9:8]
N	    B8   DMA 来源图片宽度[7:0]
N	    B9   DMA 来源图片宽度[9:8]
N	*/
N	RA8875_WriteReg(0xB4, _usWidth);		/* DMA 区块宽度 */
N	RA8875_WriteReg(0xB5, _usWidth >> 8);
N
N	RA8875_WriteReg(0xB6, _usHeight);		/* DMA 区块高度 */
N	RA8875_WriteReg(0xB7, _usHeight >> 8);
N
N	RA8875_WriteReg(0xB8, _usWidth);		/* DMA 来源图片宽度 */
N	RA8875_WriteReg(0xB9, _usWidth >> 8);
N
N	RA8875_WriteReg(0xBF, 1 << 1);			/* 选择区块模式 */
N	RA8875_WriteReg(0xBF, 1 << 1);			/* 选择区块模式 */
N
N	s_ucRA8875Busy = 1;
N	RA8875_WriteCmd(0xBF);
N	RA8875_WriteData((1 << 1) | (1 << 0));
N	while (RA8875_ReadReg_Int(0xBF) & (1 << 0));	/* 等待结束 */
N	s_ucRA8875Busy = 0;
N
N	RA8875_QuitWinMode();					/* 退出小窗口绘图模式 */
N}
N
N/*
N*********************************************************************************************************
N*	函 数 名: RA8875_DrawBMP
N*	功能说明: 在LCD上显示一个BMP位图，位图点阵扫描次序:从左到右，从上到下
N*	形    参:
N*			_usX, _usY : 图片的坐标
N*			_usHeight  :图片高度
N*			_usWidth   :图片宽度
N*			_ptr       :图片点阵指针
N*	返 回 值: 无
N*********************************************************************************************************
N*/
Nvoid RA8875_DrawBMP(uint16_t _usX, uint16_t _usY, uint16_t _usHeight, uint16_t _usWidth, uint16_t *_ptr)
N{
N	uint32_t index = 0;
N	const uint16_t *p;
N
N	/* 设置图片的位置和大小， 即设置显示窗口 */
N	RA8875_SetDispWin(_usX, _usY, _usHeight, _usWidth);
N
N	s_ucRA8875Busy = 1;
N
N	RA8875_WriteCmd(0x02); 		/* 准备读写显存 */
N
N	p = _ptr;
N	for (index = 0; index < _usHeight * _usWidth; index++)
N	{
N		/*
N			armfly : 进行优化, 函数就地展开
N			RA8875_WriteRAM(_ptr[index]);
N
N			此处可考虑用DMA操作
N		*/
N		RA8875_WriteData16(*p++);
N	}
N	s_ucRA8875Busy = 0;
N
N	/* 退出窗口绘图模式 */
N	RA8875_QuitWinMode();
N}

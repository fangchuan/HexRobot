N/*
N*********************************************************************************************************
N*	函 数 名: RA8875_SetFont
N*	功能说明: 文本模式，设置文字字体、行距和字距
N*	形    参:
N*			_ucFontType : 字体类型: RA_FONT_16, RA_FONT_24, RA_FONT_32
N*			_ucLineSpace : 行距，像素单位 (0-31)
N*			_ucCharSpace : 字距，像素单位 (0-63)
N*
N*	返 回 值: 无
N*********************************************************************************************************
N*/
Nvoid RA8875_SetFont(uint8_t _ucFontType, uint8_t _ucLineSpace, uint8_t _ucCharSpace)
N{
N	/*
N		[29H]在文字模式下，用来设定文字间的行距 (单位: 像素) 。
N		只有低5个bit有效，0-31
N	*/
N	if (_ucLineSpace >31)
N	{
N		_ucLineSpace = 31;
N	}
N	RA8875_WriteReg(0x29, _ucLineSpace);
N
N	/*
N		[2EH] 设置字符间距（像素单位，0-63），和字体（16*16，24*24，32*32）
N	*/
N	if (_ucCharSpace > 63)
N	{
N		_ucCharSpace = 63;
N	}
N	if (_ucFontType > RA_FONT_32)
N	{
N		_ucFontType = RA_FONT_16;
N	}
N	RA8875_WriteReg(0x2E, (_ucCharSpace & 0x3F) | (_ucFontType << 6));
N}

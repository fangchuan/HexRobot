N	RA8875_REG = 0x46; RA8875_RAM = _usX;
X	*(volatile uint16_t *)(((uint32_t)(0x6C000000 | 0x00000000)) + (1 << (18 + 1))) = 0x46; *(volatile uint16_t *)(((uint32_t)(0x6C000000 | 0x00000000))) = _usX;
N	RA8875_REG = 0x47; RA8875_RAM = _usX >> 8;

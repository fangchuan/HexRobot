N/*
N*********************************************************************************************************
N*	函 数 名: RA8875_SetCursor
N*	功能说明: 设置写显存的光标位置（图形模式）
N*	形    参:  _usX : X坐标; _usY: Y坐标
N*	返 回 值: 无
N*********************************************************************************************************
N*/
Nstatic void RA8875_SetCursor(uint16_t _usX, uint16_t _usY)
N{
N	if (g_LcdDirection > 1)	/* 竖屏  */
N	{
N		RA8875_WriteReg(0x46, _usY);
N		RA8875_WriteReg(0x47, _usY >> 8);
N		RA8875_WriteReg(0x48, _usX);
N		RA8875_WriteReg(0x49, _usX >> 8);
N	}
N	else	/* 横屏 */
N	{
N		RA8875_WriteReg(0x46, _usX);
N		RA8875_WriteReg(0x47, _usX >> 8);
N		RA8875_WriteReg(0x48, _usY);
N		RA8875_WriteReg(0x49, _usY >> 8);
N	}

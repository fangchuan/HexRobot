N/*
N*********************************************************************************************************
N*	函 数 名: RA8875_DrawLine
N*	功能说明: 采用RA8875的硬件功能，在2点间画一条直线。
N*	形    参:
N*			_usX1, _usY1 :起始点坐标
N*			_usX2, _usY2 :终止点Y坐标
N*			_usColor     :颜色
N*	返 回 值: 无
N*********************************************************************************************************
N*/
Nvoid RA8875_DrawLine(uint16_t _usX1 , uint16_t _usY1 , uint16_t _usX2 , uint16_t _usY2 , uint16_t _usColor)
N{
N	/* pdf 第131页
N		RA8875 支持直线绘图功能，让使用者以简易或低速的MCU 就可以在TFT 模块上画直线。先设
N		定直线的起始点REG[91h~94h] 与结束点REG[95h~98h]，直线的颜色REG[63h~65h]，然后启
N		动绘图设定REG[90h] Bit4 = 0, Bit0=0 且REG[90h] Bit7 = 1，RA8875 就会将直线的图形写入
N		DDRAM，相对的在TFT 模块上就可以显示所画的直线。
N	*/
N
N	if ((_usX1 == _usX2) && (_usY1 == _usY2))
N	{
N		RA8875_PutPixel(_usX1, _usY1, _usColor);
N		return;
N	}
N
N	if (g_LcdDirection > 1)	/* 竖屏  */
N	{
N		uint16_t temp;
N		
N		temp = _usX1;
N		_usX1 = _usY1;
N		_usY1 = temp;
N
N		temp = _usX2;
N		_usX2 = _usY2;
N		_usY2 = temp;		
N	}
N	
N	/* 设置起点坐标 */
N	RA8875_WriteReg(0x91, _usX1);
N	RA8875_WriteReg(0x92, _usX1 >> 8);
N	RA8875_WriteReg(0x93, _usY1);
N	RA8875_WriteReg(0x94, _usY1 >> 8);
N
N	/* 设置终点坐标 */
N	RA8875_WriteReg(0x95, _usX2);
N	RA8875_WriteReg(0x96, _usX2 >> 8);
N	RA8875_WriteReg(0x97, _usY2);
N	RA8875_WriteReg(0x98, _usY2 >> 8);
N
N	RA8875_SetFrontColor(_usColor);	/* 设置直线的颜色 */
N
N	s_ucRA8875Busy = 1;
N//	RA8875_WriteReg(0x90, (1 << 7) | (0 << 4) | (0 << 0));/* 开始画直线 */
N	RA8875_WriteCmd(0x90);
N	RA8875_WriteData((1 << 7) | (0 << 4) | (0 << 0));
N//	while (RA8875_ReadReg(0x90) & (1 << 7));				/* 等待结束 */
N	RA8875_WaitBusy();
N	s_ucRA8875Busy = 0;
N}
N
N/*
N*********************************************************************************************************
N*	函 数 名: RA8875_DrawRect
N*	功能说明: 采用RA8875硬件功能绘制矩形
N*	形    参:
N*			_usX,_usY:矩形左上角的坐标
N*			_usHeight :矩形的高度
N*			_usWidth  :矩形的宽度
N*			_usColor  :颜色
N*	返 回 值: 无
N*********************************************************************************************************
N*/
Nvoid RA8875_DrawRect(uint16_t _usX, uint16_t _usY, uint16_t _usHeight, uint16_t _usWidth, uint16_t _usColor)
N{
N	/*
N		RA8875 支持方形绘图功能，让使用者以简易或低速的MCU 就可以在TFT 模块上画方形。先设
N	定方形的起始点REG[91h~94h]与结束点REG[95h~98h]，方形的颜色REG[63h~65h]，然后启
N	动绘图设定REG[90h] Bit4=1, Bit0=0 且REG[90h] Bit7 = 1，RA8875 就会将方形的图形写入
N	DDRAM，相对的在TFT 模块上就可以显示所画的方形。若设定REG[90h] Bit5 = 1，则可画出一
N	实心方形 (Fill)
N
N	 ---------------->---
N	|(_usX，_usY)        |
N	V                    V  _usHeight
N	|                    |
N	 ---------------->---
N		  _usWidth
N	*/
N
N	if (g_LcdDirection > 1)	/* 竖屏  */
N	{
N		uint16_t temp;
N		
N		temp = _usX;
N		_usX = _usY;
N		_usY = temp;
N
N		temp = _usHeight;
N		_usHeight = _usWidth;
N		_usWidth = temp;		
N	}
N
N	/* 设置起点坐标 */
N	RA8875_WriteReg(0x91, _usX);
N	RA8875_WriteReg(0x92, _usX >> 8);
N	RA8875_WriteReg(0x93, _usY);
N	RA8875_WriteReg(0x94, _usY >> 8);
N
N	/* 设置终点坐标 */
N	RA8875_WriteReg(0x95, _usX + _usWidth - 1);
N	RA8875_WriteReg(0x96, (_usX + _usWidth - 1) >> 8);
N	RA8875_WriteReg(0x97, _usY + _usHeight - 1);
N	RA8875_WriteReg(0x98, (_usY + _usHeight - 1) >> 8);
N
N	RA8875_SetFrontColor(_usColor);	/* 设置颜色 */
N
N	s_ucRA8875Busy = 1;
N//	RA8875_WriteReg(0x90, (1 << 7) | (0 << 5) | (1 << 4) | (0 << 0));	/* 开始画矩形  */
N	RA8875_WriteCmd(0x90);
N	RA8875_WriteData((1 << 7) | (0 << 5) | (1 << 4) | (0 << 0));
N//	while (RA8875_ReadReg(0x90) & (1 << 7));							/* 等待结束 */
N	RA8875_WaitBusy();
N	s_ucRA8875Busy = 0;
N}
N
N/*
N*********************************************************************************************************
N*	函 数 名: RA8875_FillRect
N*	功能说明: 采用RA8875硬件功能填充一个矩形为单色
N*	形    参:
N*			_usX,_usY:矩形左上角的坐标
N*			_usHeight :矩形的高度
N*			_usWidth  :矩形的宽度
N*			_usColor  :颜色
N*	返 回 值: 无
N*********************************************************************************************************
N*/
Nvoid RA8875_FillRect(uint16_t _usX, uint16_t _usY, uint16_t _usHeight, uint16_t _usWidth, uint16_t _usColor)
N{
N	/*
N		RA8875 支持方形绘图功能，让使用者以简易或低速的MCU 就可以在TFT 模块上画方形。先设
N	定方形的起始点REG[91h~94h]与结束点REG[95h~98h]，方形的颜色REG[63h~65h]，然后启
N	动绘图设定REG[90h] Bit4=1, Bit0=0 且REG[90h] Bit7 = 1，RA8875 就会将方形的图形写入
N	DDRAM，相对的在TFT 模块上就可以显示所画的方形。若设定REG[90h] Bit5 = 1，则可画出一
N	实心方形 (Fill)
N
N	 ---------------->---
N	|(_usX，_usY)        |
N	V                    V  _usHeight
N	|                    |
N	 ---------------->---
N		  _usWidth
N	*/
N
N	if (_usWidth == 0 || _usHeight == 0)
N	{
N		return;
N	}
N
N	if (g_LcdDirection > 1)	/* 竖屏  */
N	{
N		uint16_t temp;
N		
N		temp = _usX;
N		_usX = _usY;
N		_usY = temp;
N		
N		temp = _usHeight;
N		_usHeight = _usWidth;
N		_usWidth = temp;
N	}
N	
N	/* 设置起点坐标 */
N	RA8875_WriteReg(0x91, _usX);
N	RA8875_WriteReg(0x92, _usX >> 8);
N	RA8875_WriteReg(0x93, _usY);
N	RA8875_WriteReg(0x94, _usY >> 8);
N
N	/* 设置终点坐标 */
N	RA8875_WriteReg(0x95, _usX + _usWidth - 1);
N	RA8875_WriteReg(0x96, (_usX + _usWidth - 1) >> 8);
N	RA8875_WriteReg(0x97, _usY + _usHeight - 1);
N	RA8875_WriteReg(0x98, (_usY + _usHeight - 1) >> 8);
N
N	RA8875_SetFrontColor(_usColor);	/* 设置颜色 */
N
N	s_ucRA8875Busy = 1;
N//	RA8875_WriteReg(0x90, (1 << 7) | (1 << 5) | (1 << 4) | (0 << 0));	/* 开始填充矩形  */
N	RA8875_WriteCmd(0x90);
N	RA8875_WriteData((1 << 7) | (1 << 5) | (1 << 4) | (0 << 0));
N//	while (RA8875_ReadReg(0x90) & (1 << 7));							/* 等待结束 */
N	RA8875_WaitBusy();
N	s_ucRA8875Busy = 0;
N}

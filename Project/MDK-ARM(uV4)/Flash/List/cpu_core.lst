N*                           x    x    x    x    x         1    0    0            2
N*                           :    :    :    :    :         :    :    :            :
N*                           :    :    :    :    :         :    :    :            :
N*                           x    x    x    x    1         0    0    0           59
N*                           x    x    x    1    0         0    0    0           60
N*                           x    x    1    0    0         0    0    0           61
N*                           x    1    0    0    0         0    0    0           62
N*                           1    0    0    0    0         0    0    0           63
N*                           0    0    0    0    0         0    0    0           64
N*
N*
N*               (2) For non-zero values, the returned number of contiguous, least-significant, trailing 
N*                   zero bits is also equivalent to the bit position of the least-significant set bit.
N*
N*               (3) 'val' SHOULD be validated for non-'0' PRIOR to all other counting zero calculations :
N*
N*                   (a) For assembly-optimized implementations, CPU_CntTrailZeros() returns 'val's 
N*                       number of trailing zeros via CPU's native data size, 'CPU_CFG_DATA_SIZE'.  
N*                       If the returned number of zeros exceeds CPU_CntTrailZeros64()'s 64-bit return 
N*                       data size, then the returned number of zeros must be offset by the difference 
N*                       between CPU_CntTrailZeros()'s  & CPU_CntTrailZeros64()'s return data size :
N*
N*                           nbr_trail_zeros = CPU_CntTrailZeros((CPU_DATA)val);
N*                           if (nbr_trail_zeros >  (CPU_WORD_SIZE_64  * DEF_OCTET_NBR_BITS)) {
N*                               nbr_trail_zeros -= (CPU_CFG_DATA_SIZE - CPU_WORD_SIZE_64) * DEF_OCTET_NBR_BITS;
N*                           }
N*
N*                       However, this ONLY occurs for an initial 'val' of '0' since all non-'0' 64-bit 
N*                       values would return a number of trailing zeros less than or equal to 64 bits.
N*
N*                       Therefore, initially validating all non-'0' values prior to calling assembly-
N*                       optimized CPU_CntTrailZeros() avoids having to offset the number of returned 
N*                       trailing zeros by the difference in CPU data size and 64-bit data value bits.
N*
N*                   (b) For CPU_CntTrailZeros64()'s C implementation, the final conditional statement 
N*                       calculates 'val's number of trailing zeros based on CPU_CntTrailZeros64()'s 
N*                       64-bit return data size & 'val's calculated number of lead zeros ONLY if the 
N*                       initial 'val' is non-'0' :
N*
N*                           if (val != 0u) {
N*                               nbr_trail_zeros = ((CPU_WORD_SIZE_64 * DEF_OCTET_NBR_BITS) - 1u) - nbr_lead_zeros;
N*                           } else {
N*                               nbr_trail_zeros = nbr_lead_zeros;
N*                           }
N*
N*                       Therefore, initially validating all non-'0' values avoids having to conditionally 
N*                       execute the final 'if' statement.
N*********************************************************************************************************
N*/
N/*$PAGE*/
N#if (CPU_CFG_DATA_SIZE_MAX >= CPU_WORD_SIZE_64)
X#if (8 >= 8)
NCPU_DATA  CPU_CntTrailZeros64 (CPU_INT64U  val)
N{
N#if  (!((defined(CPU_CFG_TRAIL_ZEROS_ASM_PRESENT)) && \
N        (CPU_CFG_DATA_SIZE >= CPU_WORD_SIZE_64)))
X#if  (!((1L) &&         (4 >= 8)))
N    CPU_INT64U  val_bit_mask;
N    CPU_DATA    nbr_lead_zeros;
N#endif
N    CPU_DATA    nbr_trail_zeros;
N
N
N    if (val == 0u) {                                            /* Rtn ALL val bits as zero'd (see Note #3).            */
N        return (CPU_WORD_SIZE_64 * DEF_OCTET_NBR_BITS);
X        return (8 * 8u);
N    }
N
N                                                                /* ------------------ ASM-OPTIMIZED ------------------- */
N#if ((defined(CPU_CFG_TRAIL_ZEROS_ASM_PRESENT)) && \
N     (CPU_CFG_DATA_SIZE >= CPU_WORD_SIZE_64))
X#if ((1L) &&      (4 >= 8))
S    nbr_trail_zeros = CPU_CntTrailZeros((CPU_DATA)val);
S
N#else                                                           /* ------------------- C-OPTIMIZED -------------------- */
N    val_bit_mask    = val & ((CPU_INT64U)~val + 1u);            /* Zero/clr all bits EXCEPT least-sig set bit.          */
N    nbr_lead_zeros  = CPU_CntLeadZeros64(val_bit_mask);         /* Cnt  nbr lead  0s.                                   */
N                                                                /* Calc nbr trail 0s = (nbr val bits - 1) - nbr lead 0s.*/
N    nbr_trail_zeros = ((CPU_WORD_SIZE_64 * DEF_OCTET_NBR_BITS) - 1u) - nbr_lead_zeros;
X    nbr_trail_zeros = ((8 * 8u) - 1u) - nbr_lead_zeros;
N#endif
N
N
N    return (nbr_trail_zeros);
N}
N#endif
N

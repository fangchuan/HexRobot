N/*
N*********************************************************************************************************
N*                                            CPU_TS_Init()
N*
N* Description : (1) Initialize CPU timestamp :
N*
N*                   (a) Initialize/start CPU timestamp timer                            See Note #1
N*                   (b) Initialize       CPU timestamp controls
N*
N*
N* Argument(s) : none.
N*
N* Return(s)   : none.
N*
N* Caller(s)   : CPU_Init().
N*
N* Note(s)     : (1) The following initialization MUST be sequenced as follows :
N*
N*                   (a) CPU_TS_TmrFreq_Hz     MUST be initialized prior to CPU_TS_TmrInit()
N*                   (b) CPU_TS_TmrInit()      SHOULD precede calls to all other CPU timestamp functions;
N*                                                 otherwise, invalid time measurements may be calculated/
N*                                                 returned.
N*
N*                   See also 'CPU_Init()  Note #3a'.
N*********************************************************************************************************
N*/
N
N#if ((CPU_CFG_TS_EN     == DEF_ENABLED) || \
N     (CPU_CFG_TS_TMR_EN == DEF_ENABLED))
X#if ((1u     == 1u) ||      (1u == 1u))
Nstatic  void  CPU_TS_Init (void)
N{
N#if (((CPU_CFG_TS_32_EN    == DEF_ENABLED     )  && \
N      (CPU_CFG_TS_TMR_SIZE <  CPU_WORD_SIZE_32)) || \
N     ((CPU_CFG_TS_64_EN    == DEF_ENABLED     )  && \
N      (CPU_CFG_TS_TMR_SIZE <  CPU_WORD_SIZE_64)))
X#if (((1u    == 1u     )  &&       (4 <  4)) ||      ((0u    == 1u     )  &&       (4 <  8)))
S    CPU_TS_TMR  ts_tmr_cnts;
N#endif
N
N
N                                                                /* ----------------- INIT CPU TS TMR ------------------ */
N#if (CPU_CFG_TS_TMR_EN == DEF_ENABLED)
X#if (1u == 1u)
N    CPU_TS_TmrFreq_Hz   = 0u;                                   /* Init/clr     ts tmr freq (see Note #1a).             */
N    CPU_TS_TmrInit();                                           /* Init & start ts tmr      (see Note #1b).             */
N#endif
N
N
N                                                                /* ------------------- INIT CPU TS -------------------- */
N#if (((CPU_CFG_TS_32_EN    == DEF_ENABLED     )  && \
N      (CPU_CFG_TS_TMR_SIZE <  CPU_WORD_SIZE_32)) || \
N     ((CPU_CFG_TS_64_EN    == DEF_ENABLED     )  && \
N      (CPU_CFG_TS_TMR_SIZE <  CPU_WORD_SIZE_64)))
X#if (((1u    == 1u     )  &&       (4 <  4)) ||      ((0u    == 1u     )  &&       (4 <  8)))
S    ts_tmr_cnts = CPU_TS_TmrRd();                               /* Get init ts tmr val (in ts tmr cnts).                */
N#endif
N
N#if  ((CPU_CFG_TS_32_EN    == DEF_ENABLED)  && \
N      (CPU_CFG_TS_TMR_SIZE <  CPU_WORD_SIZE_32))
X#if  ((1u    == 1u)  &&       (4 <  4))
S    CPU_TS_32_Accum   = 0u;                                     /* Init 32-bit accum'd ts.                              */
S    CPU_TS_32_TmrPrev = ts_tmr_cnts;                            /* Init 32-bit ts prev tmr val.                         */
N#endif
N
N#if  ((CPU_CFG_TS_64_EN    == DEF_ENABLED)  && \
N      (CPU_CFG_TS_TMR_SIZE <  CPU_WORD_SIZE_64))
X#if  ((0u    == 1u)  &&       (4 <  8))
S    CPU_TS_64_Accum   = 0u;                                     /* Init 64-bit accum'd ts.                              */
S    CPU_TS_64_TmrPrev = ts_tmr_cnts;                            /* Init 64-bit ts prev tmr val.                         */
N#endif
N}
N#endif
